[ T-Cross ]
    title = [t-cross] in xyz mesh
     mesh =  xyz            # mesh type is xyz scoring mesh
   x-type =    2            # x-mesh is linear given by xmin, xmax and nx
     xmin =  -30.00000      # minimum value of x-mesh points
     xmax =   20.00000      # maximum value of x-mesh points
#    xdel =   50.00000      # mesh width of x-mesh points
       nx =      1          # number of x-mesh points
#    data = ( x(i), i = 1, nx + 1 )
#          -3.00000E+01  2.00000E+01
   y-type =    2            # y-mesh is linear given by ymin, ymax and ny
     ymin =  -35.00000      # minimum value of y-mesh points
     ymax =   15.00000      # maximum value of y-mesh points
#    ydel =   50.00000      # mesh width of y-mesh points
       ny =      1          # number of y-mesh points
#    data = ( y(i), i = 1, ny + 1 )
#          -3.50000E+01  1.50000E+01
   z-type =    2            # z-mesh is linear given by zmin, zmax and nz
     zmin =   27.00000      # minimum value of z-mesh points
     zmax =   77.00000      # maximum value of z-mesh points
#    zdel =   50.00000      # mesh width of z-mesh points
       nz =      1          # number of z-mesh points
#    data = ( z(i), i = 1, nz + 1 )
#           2.70000E+01  7.70000E+01
   e-type =    2            # e-mesh is linear given by emin, emax and ne
     emin =   0.000000      # minimum value of e-mesh points
     emax =   200.0000      # maximum value of e-mesh points
#    edel =   200.0000      # mesh width of e-mesh points
       ne =      1          # number of e-mesh points
#    data = ( e(i), i = 1, ne + 1 )
#           0.00000E+00  2.00000E+02
  eng2let =    0            # (D=0) Flag to convert energy to LET, 0: not convert, 1: convert
     unit =    1            # unit is [1/cm^2/source]
     axis =  eng            # axis of output
     file = total_neutron_current.out  # file name of output for the above axis
   output = current         # surface crossing current spectrum
     part =  neutron  
# kf/name :  2112     
#    used :        main (  %)        temp (  %)       total (  %)
#  memory :          12 (  0)           0 (  0)          12 (  0)

